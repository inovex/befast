{
  "name": "gatsby-starter-blog",
  "description": "Starter Gatsby Blog",
  "version": "1.0.0",
  "author": "Jacob Cofman <jcofman@inovex.de>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby-starter-blog/issues"
  },
  "dependencies": {
    "gatsby": "2.0.14",
    "gatsby-plugin-feed": "2.0.6",
    "gatsby-plugin-google-analytics": "2.0.6",
    "gatsby-plugin-offline": "2.0.5",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-sharp": "2.0.5",
    "gatsby-plugin-typography": "2.2.0",
    "gatsby-remark-copy-linked-files": "2.0.5",
    "gatsby-remark-images": "2.0.3",
    "gatsby-remark-prismjs": "3.0.1",
    "gatsby-remark-responsive-iframe": "2.0.5",
    "gatsby-remark-smartypants": "2.0.5",
    "gatsby-source-filesystem": "2.0.1",
    "gatsby-transformer-remark": "2.1.5",
    "gatsby-transformer-sharp": "2.1.3",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typeface-merriweather": "0.0.54",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "@octokit/rest": "^15.12.1",
    "bundlesize": "^0.17.0",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.0",
    "lighthouse-ci": "git+https://github.com/ebidel/lighthouse-ci.git",
    "now": "^11.4.6",
    "prettier": "^1.14.3",
    "serve": "^10.0.2",
    "size-limit": "^0.20.0",
    "webpagetest": "^0.3.9"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "now-build": "echo 'Built on Travis CI'",
    "lh": "lighthouse-ci --score=90 --performance=90 --pwa=90 --accessibility=90 --best-practice=90 --seo=90",
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "bundlesize && size-limit",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "start": "serve public/",
    "build": "gatsby build",
    "deploy": "now --public -e NODE_ENV=production --token $NOW_TOKEN --yarn",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "bundlesize": [
    {
      "path": "./public/app*.js",
      "maxSize": "170 kB"
    },
    {
      "path": "./public/component*.js",
      "maxSize": "55 kB"
    },
    {
      "path": "./public/*.css",
      "maxSize": "10 kB"
    }
  ],
  "size-limit": [
    {
      "path": "./public/*.js",
      "webpack": false,
      "limit": "250 KB"
    }
  ]
}
